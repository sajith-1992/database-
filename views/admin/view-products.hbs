<section>
    <div class="container>
       
     <a herf="./admin/addd-product>add product</a>
       
        
    <table class="table table table-dark"">
  <thead>
    <tr>
      <th scope=" col">#</th>
        <th scope="col">Name</th>
        <th scope="col">category</th>
        <th scope="col">discription</th>
        <th scope="col">image</th>

        </tr>
        </thead>

        <tbody>
            {{#each products}}
            <tr>
                <th scope="raw">1</th>
                <td>{{this.name}}</td>
                <td>{{this.category}}</td>
                <td>{{this.discription}}</td>
                <td><img src="{{this.image}}" alt="" style="width:50px;height:50px;"></td>
            </tr>
            {{/each}}

        </tbody>
    </table>
    </div>

</section>